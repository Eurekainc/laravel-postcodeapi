{
  "name": "nickurt/laravel-postcodeapi",
  "description": "Universal PostcodeApi for Laravel 6.x/7.x",
  "keywords": [
    "postcodeapi",
    "postcode",
    "api",
    "universal",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "laravel/framework": "^6.0|^7.0",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "orchestra/testbench": "^4.0|^5.0",
    "cache/array-adapter": "^1.0"
  },
  "suggest": {
    "cache/array-adapter": "Required to use for the CacheProvider component (^1.0)."
  },
  "autoload": {
    "psr-4": {
      "nickurt\\PostcodeApi\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "nickurt\\PostcodeApi\\tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "nickurt\\PostcodeApi\\ServiceProvider"
      ],
      "aliases": {
        "PostcodeApi": "nickurt\\PostcodeApi\\Facades\\PostcodeApi"
      }
    }
  }
}
